<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>SimpleLoginWeb</display-name>
  <welcome-file-list>
    <welcome-file>welcome.jsp</welcome-file>
  </welcome-file-list>
  <session-config>
    <session-timeout>1</session-timeout>
  </session-config>
  <servlet>
    <servlet-name>login</servlet-name>
    <jsp-file>/login.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>register</servlet-name>
    <jsp-file>/register.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>register</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>success</servlet-name>
    <jsp-file>/success.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>success</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>logging</servlet-name>
    <servlet-class>checklogging.Account</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>logging</servlet-name>
    <url-pattern>/logging</url-pattern>
  </servlet-mapping>

  <servlet>
  	<servlet-name>newaccount</servlet-name>
  	<servlet-class>registration.NewAccount</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>newaccount</servlet-name>
  	<url-pattern>/newaccount</url-pattern>
  </servlet-mapping>
</web-app>